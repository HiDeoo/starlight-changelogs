---
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro'
import type { ChangelogProps } from '../libs/routing'
import Entry from './Entry.astro'

type Props = ChangelogProps

const { entries, pagination } = Astro.props

// TODO(HiDeoo) title
// TODO(HiDeoo) headings / h1
// TODO(HiDeoo) heading ID
---

<StarlightPage frontmatter={{ title: 'My custom page', prev: pagination.prev, next: pagination.next }}>
  {
    entries.map((entry) => (
      <>
        <h1>{entry.data.title}</h1>
        <Entry {entry} />
      </>
    ))
  }
</StarlightPage>
